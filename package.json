{
    "name": "foody-web-app",
    "version": "1.0.0",
    "description": "Foody single page application",
    "private": true,
    "scripts": {
        "build": "webpack --mode=production --config build/webpack.prod.config.ts --env server=prod",
        "devTestServer": "webpack serve --config build/webpack.dev.config.ts --env server=testServer",
        "devStopLight": "webpack serve --config build/webpack.dev.config.ts --env server=stopLight",
        "devLocal": "concurrently --kill-others \"node testServer/index.js\" \"webpack serve --config ./build/webpack.dev.config.ts --env server=local\"",
        "openapi:generateFront": "openapi-generator-cli generate -g typescript-axios -i ./openapi.yaml -o ./src/api/ -c ./openapi-config.yaml",
        "openapi:generateBack": "openapi-generator-cli generate -g nodejs-express-server -i ./openapi.yaml -o ./testServer/ -c ./openapi-config.yaml",
        "prettier": "prettier --write ."
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/PolyanaTeam/foody-web-app.git"
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/PolyanaTeam/foody-web-app/issues"
    },
    "homepage": "https://github.com/PolyanaTeam/foody-web-app#readme",
    "devDependencies": {
        "@babel/preset-env": "^7.16.5",
        "@babel/preset-typescript": "^7.16.5",
        "@openapitools/openapi-generator-cli": "^2.4.24",
        "@types/css-minimizer-webpack-plugin": "^3.2.1",
        "@types/jest": "^27.0.3",
        "@types/node": "^17.0.8",
        "@types/node-sass": "^4.11.2",
        "@types/react": "^17.0.37",
        "@types/react-dom": "^17.0.11",
        "@types/react-router-dom": "^5.3.2",
        "@types/webpack": "^5.28.0",
        "@types/webpack-dev-server": "^4.7.2",
        "autoprefixer": "^10.4.0",
        "concurrently": "^6.5.1",
        "copy-webpack-plugin": "^10.2.1",
        "cors": "^2.8.5",
        "css-loader": "^6.5.1",
        "css-minimizer-webpack-plugin": "^3.2.0",
        "express": "^4.17.1",
        "html-webpack-plugin": "^5.5.0",
        "jest": "^27.4.5",
        "jest-fetch-mock": "^3.0.3",
        "mini-css-extract-plugin": "^2.4.5",
        "postcss-loader": "^6.2.1",
        "prettier": "2.5.1",
        "sass": "^1.44.0",
        "sass-loader": "^12.4.0",
        "ts-loader": "^9.2.6",
        "ts-node": "^10.4.0",
        "typescript": "^4.5.4",
        "typescript-plugin-css-modules": "^3.4.0",
        "webpack": "^5.65.0",
        "webpack-cli": "^4.9.1",
        "webpack-dev-server": "^4.6.0",
        "webpack-merge": "^5.8.0"
    },
    "dependencies": {
        "axios": "^0.24.0",
        "js-cookie": "^3.0.1",
        "react": "^17.0.2",
        "react-dom": "^17.0.2",
        "react-indiana-drag-scroll": "^2.1.0",
        "react-input-mask": "^2.0.4",
        "react-router-dom": "^6.2.2",
        "react-toggle": "^4.1.2",
        "scroll": "^3.0.1"
    }
}
